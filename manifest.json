{
    "manifest_version": 2,
    "name": "NSubscriber",
    "version": "1.0",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["https://nhentai.net/*"],
        "js": ["lib/jquery-3.3.1.min.js", "content.js"]
    }],
    "permissions": ["storage", "activeTab"],
    "web_accessible_resources": [
        "img/delete.png"
    ]
}